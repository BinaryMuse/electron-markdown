{
  "name": "electron-markdown",
  "version": "0.8.1",
  "description": "Convert GitHub-flavored Markdown to HTML with a few extras",
  "repository": "https://github.com/BinaryMuse/electron-markdown",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "prettier --check \"./**/*.{ts,js,yml}\"",
    "lint:fix": "yarn lint --write",
    "test": "mocha && npm run tsd",
    "tsd": "tsd"
  },
  "keywords": [
    "markdown",
    "gfm",
    "cmark",
    "cmark-gfm"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "Michelle Tilley <michelle@michelletilley.net>",
  "license": "MIT",
  "tsd": {
    "directory": "test"
  },
  "dependencies": {
    "cmark-gfm": "^0.8.3",
    "mixin-deep": "^2.0.1",
    "rehype": "^11.0.0",
    "rehype-autolink-headings": "^4.0.0",
    "rehype-highlight": "^4.0.0",
    "rehype-slug": "^3.0.0",
    "remark-gemoji-to-emoji": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "prettier": "^2.0.5",
    "cheerio": "^1.0.0-rc.3",
    "mocha": "^8.0.1",
    "tsd": "^0.13.1",
    "typescript": "^4.0.2"
  }
}
